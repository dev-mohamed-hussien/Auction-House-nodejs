/* animation preloading */



.loading-bar::before{
  content: '';
  width: 68px;
  height: 2px;
  background: #0073b1;
  position: absolute;
  left: -70px;
  animation: bluebar 1.5s infinite ease;
}


@keyframes bluebar{
  50%{
    left: 96px;
    left: 100%;
  }
}













.se-pre-con {
   background: url() center no-repeat #fff;
 }
 .no-js #loader {
   display: none;
 }
 .js #loader {
   display: block;
